{
  "name": "eln",
  "version": "0.1.0",
  "author": "Howonsoft",
  "main": "electron/electron.js",
  "private": true,
  "scripts": {
    "dev": "vite --port 8080",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "wait-on tcp:3000 && cross-env IS_DEV=true electron .",
    "electron:pack": "electron-builder --dir",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"npm run electron\"",
    "electron:builder": "electron-builder",
    "build:for:electron": "cross-env ELECTRON=true vite build",
    "app:build": "npm build:for:electron && npm electron:builder",
    "json-server": "json-server --watch db.json --port 3005 --no-cors",
    "remove_water_mark_gojs": "cpx \"refs/gojs.js\" node_modules/.vite"
  },
  "build": {
    "appId": "com.my-website.my-app",
    "productName": "MyApp",
    "copyright": "Copyright Â© 2022 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  },
  "dependencies": {
    "@popperjs/core": "^2.11.4",
    "@vue/composition-api": "^1.6.2",
    "ag-grid-community": "^25.3.0",
    "ag-grid-vue3": "^25.3.1",
    "axios": "^0.21.1",
    "bootstrap": "^5.1.0",
    "bootstrap-social": "^5.1.1",
    "bootstrap-vue": "^2.21.2",
    "bootstrap-vue-3": "^0.1.6",
    "chart.js": "^3.7.1",
    "click-outside-vue3": "^4.0.1",
    "core-js": "^3.6.5",
    "es6-promise": "^4.2.8",
    "filepond": "^4.30.3",
    "filepond-plugin-file-validate-type": "^1.2.6",
    "filepond-plugin-image-exif-orientation": "^1.0.11",
    "filepond-plugin-image-preview": "^4.6.10",
    "flag-icon-css": "^3.5.0",
    "gojs": "^2.2.2",
    "jexcel": "^4.6.1",
    "jsme-editor": "^2021.7.13",
    "jspreadsheet-ce": "^4.10.1",
    "jsuites": "^4.10.1",
    "mathlive": "^0.69.11",
    "pinia": "^2.0.14",
    "vee-validate": "^4.5.8",
    "vue": "^3.2.25",
    "vue-chart-3": "^3.1.8",
    "vue-class-component": "^8.0.0-rc.1",
    "vue-filepond": "^7.0.2",
    "vue-i18n": "^9.1.7",
    "vue-input-text-to-tags": "^1.0.1",
    "vue-media-upload": "^1.0.3",
    "vue-plugin-load-script": "^2.1.1",
    "vue-router": "^4.0.10",
    "vue-simple-calendar": "^6.0.4",
    "vue3-cookies": "^1.0.1",
    "@vuepic/vue-datepicker": "^3.5.2",
    "vue3-datepicker": "^0.3.2",
    "vue3-emoji-picker": "^1.1.1",
    "vuedraggable": "^4.1.0",
    "vueperslides": "^3.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "sass": "^1.49.7",
    "vite": "^2.7.2",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.5",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "wait-on": "^6.0.0"
  }
}
